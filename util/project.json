{
  "name": "util",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "util/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/util",
        "main": "util/src/index.ts",
        "tsConfig": "util/tsconfig.lib.json",
        "assets": ["util/*.md"]
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["util/**/*.ts", "util/package.json"]
      }
    },
    "test": {
      "executor": "@nx/vite:test",
      "outputs": ["{options.reportsDirectory}"],
      "options": {
        "passWithNoTests": true,
        "reportsDirectory": "../coverage/util"
      }
    }
  },
  "tags": []
}
